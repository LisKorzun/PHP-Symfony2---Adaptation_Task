{% extends '::base.html.twig' %}

{% block title %}Contacts{% endblock %}

{% block body -%}

    <h1>Contacts</h1>

    <table class="table table-bordered">
        <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Subject</th>
            <th>Body</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>{{ data.name }}</td>
            <td>{{ data.email }}</td>
            <td>{{ data.subject }}</td>
            <td>{{ data.body }}</td>
        </tr>
        </tbody>
    </table>

{% endblock %}
